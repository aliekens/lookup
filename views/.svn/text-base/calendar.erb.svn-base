BEGIN:VCALENDAR
VERSION:2.0
X-WR-CALDESC;VALUE=TEXT:Upcoming flybys of ISS
X-WR-CALNAME;VALUE=TEXT:ISS flybys
CALSCALE:GREGORIAN
METHOD:PUBLISH
PRODID:http://anthony.liekens.net

<% @results.each do |flyby| %>
BEGIN:VEVENT
DTSTART:<%=flyby.date%>T<%=flyby.starttime%>Z
DTEND:<%=flyby.date%>T<%=flyby.endtime%>Z
SUMMARY:ISS <%=flyby.magnitude%>
LOCATION:<%=flyby.location%><% if @alarm %>
BEGIN:VALARM
ACTION:AUDIO
TRIGGER:-PT<%=@alarmtime%>M
END:VALARM<% end %>
URL:<%=flyby.url%>
END:VEVENT
<% end %>

END:VCALENDAR
